<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/frameLayout4"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".CDCInfoFragment">

    <ImageView
        android:id="@+id/imageViewSleepCDC"
        android:layout_width="1080px"
        android:layout_height="476px"
        app:layout_constraintHorizontal_bias="0.0"
        tools:src="@tools:sample/avatars" />

    <ImageView
        android:id="@+id/imageViewBurnedCDC"
        android:layout_width="1080px"
        android:layout_height="1378px"
        android:layout_marginTop="8dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/imageViewSleepCDC"
        tools:src="@tools:sample/avatars" />

</android.support.constraint.ConstraintLayout>